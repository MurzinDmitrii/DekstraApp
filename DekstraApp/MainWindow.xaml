<Window x:Class="DekstraApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DekstraApp"
        mc:Ignorable="d"
        Icon="/icon.ico"
        Title="Дейкстра" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Menu>
                <MenuItem Header="О разработчиках" x:Name="DeveloperMenu" Click="DeveloperMenu_Click"/>
                <MenuItem Header="Справка" x:Name="HelpMenu" Click="HelpMenu_Click"/>
            </Menu>
        </StackPanel>
        <StackPanel Margin="20">
            <Frame x:Name="MainFrame"/>
        </StackPanel>
    </Grid>
</Window>
